{
    "extends": ["airbnb", "prettier", "prettier/react"],
    "parser": "babel-eslint",
    "rules": {
        "arrow-body-style": "off",
        "comma-dangle": "off",
        "jsx-a11y/anchor-is-valid": "warn",
        "indent": ["error", 4],
        "react/forbid-prop-types": "warn",
        "no-unused-vars": "warn"
    },
    "globals": {
        "alert": true,
        "document": true,
        "window": true
    }
}
