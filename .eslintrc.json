{
    "extends": "airbnb",
    "parser": "babel-eslint",
    "rules": {
        "arrow-body-style": "off",
        "comma-dangle": "off",
        "indent": ["error", 4],
        "no-unused-vars": "warn"
    },
    "globals":{
        "alert": true,
        "document": true,
        "window": true
    }
}